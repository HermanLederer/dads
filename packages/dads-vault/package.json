{
  "name": "dads-vault",
  "version": "0.1.0",
  "description": "Store user data in a secure decentralized storage",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently -n tsup,node \"tsup src/index.ts --watch\" \"nodemon dist/index.js\"",
    "build": "npm run prisma:generate && tsup src/index.ts",
    "prisma:migrate": "pnpx prisma migrate dev --name init",
    "prisma:studio": "pnpx prisma studio",
    "prisma:generate": "pnpx prisma generate"
  },
  "author": "HermanLederer",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.6.0",
    "tsup": "^5.10.0"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "express": "^4.17.1"
  }
}
