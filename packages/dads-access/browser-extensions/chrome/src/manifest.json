{
  "name": "DADS Access",
  "description": "DADS api for web applications in Chrome",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background/main.ts"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/main.ts"]
    }
  ]
}
